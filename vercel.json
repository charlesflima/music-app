{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "DB_USERNAME": "@db_username",
    "DB_PASSWORD": "@db_password",
    "DB_DATABASE": "@db_database",
    "DB_HOST": "@db_host",
    "PORT": "3000"
  }
}


  
  
  